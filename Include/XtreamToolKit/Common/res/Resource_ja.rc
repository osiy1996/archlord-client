XTP_IDR_MENU_HEADERFOOTER_FORMATS MENU DISCARDABLE 
BEGIN
    POPUP "HF"
    BEGIN
        MENUITEM "&&d   短い形式",    XTP_ID_HF_FORMAT_D1
        MENUITEM "&&D   長い形式",     XTP_ID_HF_FORMAT_D2
        MENUITEM SEPARATOR
        MENUITEM "&&t   時間",                 XTP_ID_HF_FORMAT_T1
        MENUITEM "&&T   時間 (24 時間)",       XTP_ID_HF_FORMAT_T2 
        MENUITEM SEPARATOR
        MENUITEM "&&p   ページ番号",          XTP_ID_HF_FORMAT_P1
        MENUITEM "&&P   ページ数",      XTP_ID_HF_FORMAT_P2
        MENUITEM SEPARATOR
        MENUITEM "&&b   左揃え &&b 中央揃え &&b 右揃え", XTP_ID_HF_FORMAT_B
        MENUITEM SEPARATOR
        MENUITEM "&&w   ウィンドウ タイトル",         XTP_ID_HF_FORMAT_W
        MENUITEM "&&&&  アンパサンド (&&)",       XTP_ID_HF_FORMAT_UMP
        MENUITEM "\\n   改行",             XTP_ID_HF_FORMAT_N
    END
END

