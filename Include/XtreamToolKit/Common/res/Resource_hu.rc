    PUSHBUTTON      "Nagyítás",AFX_ID_PREVIEW_ZOOMIN,186,2,44,12,BS_FLAT
    PUSHBUTTON      "Kicsinyítés",AFX_ID_PREVIEW_ZOOMOUT,232,2,44,12,BS_FLAT
    PUSHBUTTON      "Bezárás",AFX_ID_PREVIEW_CLOSE,278,2,44,12,BS_FLAT
END


