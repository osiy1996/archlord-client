XT_IDM_POPUP MENU DISCARDABLE 
BEGIN
    POPUP "RClickOutlookBar"
    BEGIN
        MENUITEM "작은 아이콘",                XT_IDC_SMALLICON
        MENUITEM "큰 아이콘",                XT_IDC_LARGEICON
    END
    POPUP "RClickHexEdit"
    BEGIN
        MENUITEM "실행 취소",                       ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "잘라내기",                        ID_EDIT_CUT
        MENUITEM "복사",                       ID_EDIT_COPY
        MENUITEM "붙여넣기",                      ID_EDIT_PASTE
        MENUITEM "삭제",                     ID_EDIT_CLEAR
        MENUITEM SEPARATOR
        MENUITEM "모두 선택",                 ID_EDIT_SELECT_ALL
    END
    POPUP "RClickHeader"
    BEGIN
        MENUITEM "오름차순 정렬",              XT_IDC_SORTASC
        MENUITEM "내림차순 정렬",             XT_IDC_SORTDSC
        MENUITEM SEPARATOR
        POPUP "맞춤"
        BEGIN
            MENUITEM "왼쪽 맞춤",                  XT_IDC_ALIGNLEFT
            MENUITEM "가운데 맞춤",                XT_IDC_ALIGNCENTER
            MENUITEM "오른쪽 맞춤",                 XT_IDC_ALIGNRIGHT
        END
    END
END

