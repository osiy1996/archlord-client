XT_IDM_POPUP MENU DISCARDABLE 
BEGIN
    POPUP "RClickOutlookBar"
    BEGIN
        MENUITEM "Küçük Simge",                XT_IDC_SMALLICON
        MENUITEM "Büyük Simge",                XT_IDC_LARGEICON
    END
    POPUP "RClickHexEdit"
    BEGIN
        MENUITEM "Geri Al",                       ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "Kes",                        ID_EDIT_CUT
        MENUITEM "Kopya",                       ID_EDIT_COPY
        MENUITEM "Yapýþtýr",                      ID_EDIT_PASTE
        MENUITEM "Delete",                     ID_EDIT_CLEAR
        MENUITEM SEPARATOR
        MENUITEM "Tümünü &Seç",                 ID_EDIT_SELECT_ALL
    END
    POPUP "RClickHeader"
    BEGIN
        MENUITEM "Artan Sýrada",              XT_IDC_SORTASC
        MENUITEM "Azalan Sýrada",             XT_IDC_SORTDSC
        MENUITEM SEPARATOR
        POPUP "Hizalama"
        BEGIN
            MENUITEM "Sola Hizala",                  XT_IDC_ALIGNLEFT
            MENUITEM "Dikey Ortaya Hizala",                XT_IDC_ALIGNCENTER
            MENUITEM "Saða Hizala",                 XT_IDC_ALIGNRIGHT
        END
    END
END

